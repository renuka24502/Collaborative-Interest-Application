

<html>
<body>
<section style="background-image:url(img/Back1.jpg);height: 675px">
 <form method = 'get' action = 'SubmitReviews'>
		 <hr>
				<h1>Reviews:</h1>
				 <table align="center">	
				 
				 <tr>
				<td>&nbsp;</td>
				</tr>
				 
				 <tr>
				 <td> Product Model Name :-</td> 
				 <td><input type='text' name='ModelName' required></td> 
				 </tr> 
				 
				 <tr>
				<td>&nbsp;</td>
				</tr>
				 
				 <tr> 
				 <td> Product Code :-</td> 
				 <td> <input type='text' name='productcode' required></td> 
				 </tr> 
				 
				 <tr>
				<td>&nbsp;</td>
				</tr>
				
				 <tr> 
				 <td> Rental Price :-</td> 
				 <td><input type='text' name='rentalprice' required> </td> 
				 </tr> 
	
			
				
				<tr>
				<td>&nbsp;</td>
				</tr>
				  
		
			 <tr> 
				 <td> User Id:-</td> 
				 <td><input type='text' name='userid' /></td> 
				 </tr> 
				 
				<tr>
				<td>&nbsp;</td>
				</tr>
			 <tr> 
				 <td> User Age:-</td> 
				 <td><input type='text' name='age'/></td> 
				 </tr> 
	<tr>
				<td>&nbsp;</td>
				</tr>
			 <tr> 
				 <td> User Gender:-</td> 
				 <td><select name='gender'><option value='Male' selected>Male</option><option value='Female'>Female</option></select></td> 
				 </tr> 
    <tr>
				<td>&nbsp;</td>
				</tr>
			 <tr> 
				 <td> User Occupation:-</td> 
				 <td><input type='text' name='occupation'/></td> 
				 </tr> 
			
			<tr>
				<td>&nbsp;</td>
				</tr>
             <tr> 
				 <td> Review Rating:-</td> 
				 <td><select name='rating'><option value='1' >1</option><option value='2'>2</option><option value='3'>3</option><option value='4'>4</option><option value='5' selected>5</option></select></td> 
				 </tr> 
				
				<tr>
				<td>&nbsp;</td>
				</tr>
				
				 <tr> 
				 <td> Review Date-</td> 
				 <td><input type='text' name='reviewdate'/></td> 
				 </tr> 
				 
				<tr>
				<td>&nbsp;</td>
				</tr>
				 
				 <tr> 
				 <td> Review Text:&nbsp;&nbsp;</td> 
				 <td><textarea name='review' rows='4' cols='50'/></textarea></td> 
				 </tr> 
				 
				 <tr><td align='left' ><input type='submit' value='Submit'></td></tr> 
			 
			 </table> 
			
			</form>
     
</body>
</html> 

